{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.5/MicrosoftTeams.schema.json",
  "manifestVersion": "1.5",
  "version": "1.0.0",
  "id": "c683b4d3-97de-4331-8340-63fcd93b47e6",
  "packageName": "com.microsoft.teams.scrumsforchannels",
  "developer": {
    "name": "<<companyName>>",
    "websiteUrl": "<<websiteUrl>>",
    "privacyUrl": "<<privacyUrl>>",
    "termsOfUseUrl": "<<termsOfUseUrl>>"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "Scrums for Channels",
    "full": "Scrums for Channels"
  },
  "description": {
    "short": "Scrums for Channelsはチーム内のあなたのスクラム ステータスを更新します。",
    "full": "Scrums for Channels はスクラムを実現するためのシンプルなツールで非同期で実行されます。簡単に日々のアップデートの共有を実現するために用意されています。"
  },
  "accentColor": "#FFCCD8",
  "bots": [
    {
      "botId": "<<botId>>",
      "scopes": [
        "team"
      ],
      "commandLists": [
        {
          "scopes": [
            "team"
          ],
          "commands": [
            {
              "title": "設定を開く",
              "description": "タスクの設定を開きスクラムを追加または更新する。"
            },
            {
              "title": "ヘルプ",
              "description": "実行できる動作を確認"
            }
          ]
        }
      ],
      "supportsFiles": false,
      "isNotificationOnly": false
    }
  ],
  "validDomains": [
    "<<valid domains>>"
  ]
}